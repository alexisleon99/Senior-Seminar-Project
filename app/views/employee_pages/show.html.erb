<p id="notice"><%= notice %></p>
<div class="col-lg-12 text-center">
  <h1 class="mt-5"> Employee Travel and Expense Forms</h1>
</div>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Department</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Location</th>
      <th>Flight price</th>
      <th>Hotel price</th>
      <th>Mileage</th>
      <th>Transportation</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

<h2 class="mt-5"> Travel Form</h2>
  <table>

  <tbody>
    <% @travel_forms.each do |travel_form| %>
    <div>
      <tr>
      <td> Travel form </td>
        <td><%= travel_form.id %></td>
        <td><%= link_to 'Review Travel Form', travel_form %></td>
      </tr>
    </div>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Travel Form', new_travel_form_path %>
<h2 class="mt-5"> Expense Reports </h2>
<table>
    <tbody>
    <% @expense_reports.each do |expense_report| %>
      <tr>
        <td> Expense Form </td>
        <td><%= expense_report.id %></td>
        <td><%= link_to 'Review Expense Report', expense_report %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<%= link_to 'New Travel Form', new_expense_report_path %>
</div>
